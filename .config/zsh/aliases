alias ls='ls --color=auto'
alias grep='grep --color=auto'
alias ll='ls -l'

alias gs='git status'
alias gss='git status -s'
alias ga='git add'
alias gp='git push'
alias gpo='git push origin'
alias gc='git commit'
alias gd='git diff'
alias gl='git log'
alias gr='git remote'
alias grs='git remote show'
alias gplo='git pull origin'
